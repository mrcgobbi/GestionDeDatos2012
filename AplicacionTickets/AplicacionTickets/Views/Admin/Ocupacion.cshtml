@model IEnumerable<AplicacionTickets.Models.eOcupacion>

@{
    ViewBag.Title = "Ocupacion";
}


<ul id="submenu">
    <li>@Html.ActionLink(" ( DESC ) ", "Ocupacion", new { or = "0" })</li>  
    <li>@Html.ActionLink(" ( ASC ) ", "Ocupacion", new { or = "1"})</li>
</ul>



<h2>@ViewBag.Mensaje</h2>

<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.e.Titulo)
        </th>
        <th>
            Artista
        </th>
        <th>
            Lugar
        </th>
        <th>
            @Html.DisplayNameFor(model => model.e.FechaHora)
        </th>
        <th>
            Porcentaje de Ocupacion
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.e.Titulo)
        </td>       
        <td>
            @Html.DisplayFor(modelItem => item.e.Artista.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.e.Lugar.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.e.FechaHora)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ocup)%
        </td>
        <td>
            @Html.ActionLink("Detalles", "Details", "Espectaculo", new { id=item.e.EspectaculoId }, null)
        </td>
    </tr>
}

</table>

<div>
    @Html.ActionLink("Volver", "Index", "Admin")
</div>
